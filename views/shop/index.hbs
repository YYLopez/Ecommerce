<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>

{{# each products}}
    <div class="row">
        {{# each this}}
            <div class="col-sm-6 col-md-4">
                <div class="thumbnail">
                    <img src="https://upload.wikimedia.org/wikipedia/en/5/5e/Gothiccover.png" alt="..."
                         class="img-responsive">
                    <div class="caption">
                        <h3>{{this.title}}</h3>
                        <p> {{this.description}}</p>
                        <div class="clearfix">
                            <div class="price pull-left">{{this.price}}</div>
                            <a href="#" class="btn btn-success pull-right" role="button">Ad to Shopping Cart</a>
                        </div>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
{{/each}}